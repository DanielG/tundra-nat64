# 0 -> the number of cores your device has
program.translator_threads = 8
# Empty -> do not chroot
program.chroot_dir = /root/tundra-chroot
# Empty -> do not drop user
program.privilege_drop_user = tundra
# Empty -> do not drop group
program.privilege_drop_group = tundra



# Either 'inherited-fds' or 'tun'
io.mode = tun
# Empty -> '/dev/net/tun'
io.tun.device_path =
# Most not be empty
io.tun.interface_name = tundra
# Empty -> do not set owner user
io.tun.owner_user = tundra
# Empty -> do not set owner group
io.tun.owner_group = tundra



translator.prefix = 64:ff9b::
translator.ipv4 = 192.168.64.2
translator.ipv6 = fd00:6464::2
translator.allow_translation_of_private_ips = yes

translator.ipv4.outbound_mtu = 1500
translator.ipv6.outbound_mtu = 1500

translator.6to4.copy_dscp_and_ecn = yes
translator.4to6.copy_dscp_and_ecn = yes

translator.checksum.check_ipv4_header = yes
translator.checksum.check_icmpv4 = yes
translator.checksum.check_icmpv6 = yes
translator.checksum.check_tcp = yes
translator.checksum.check_udp = yes



router.ipv4 = 192.168.64.1
router.ipv6 = fd00:6464::1
