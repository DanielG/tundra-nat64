TUNDRA_IPV4_SUBNET="10.64.64.0/24"
TUNDRA_TRANSLATOR_IPV6="fd64:6464::2"
TUNDRA_TRANSLATOR_PREFIX="64:ff9b::/96"
TUNDRA_INTERFACE="tundra"
LAN_INTERFACE="br-lan"
WAN_INTERFACE="eth0.2"


/usr/sbin/ip6tables -t nat -A POSTROUTING -d $TUNDRA_TRANSLATOR_PREFIX -o $TUNDRA_INTERFACE -j SNAT --to-source=$TUNDRA_TRANSLATOR_IPV6
/usr/sbin/ip6tables -A FORWARD -d $TUNDRA_TRANSLATOR_PREFIX -i $LAN_INTERFACE -o $TUNDRA_INTERFACE -j ACCEPT
/usr/sbin/iptables -A FORWARD -s $TUNDRA_IPV4_SUBNET -i $TUNDRA_INTERFACE -o $WAN_INTERFACE -j ACCEPT
